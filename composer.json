{
    "name": "robiiinos/laravel-time",
    "description": "Register a '/time' endpoint to your Laravel API.",
    "homepage": "https://github.com/robiiinos/laravel-time",
    "license": "MIT",
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^6.5",
        "phpunit/phpunit": "^9.4",
        "vimeo/psalm": "^4.2"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Robiiinos\\LaravelTime\\LaravelTimeServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Robiiinos\\LaravelTime\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Robiiinos\\LaravelTime\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "php-cs-fixer": "vendor/bin/php-cs-fixer fix --config=.php_cs.dist --dry-run",
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test:coverage": "vendor/bin/phpunit --coverage-html coverage"
    }
}
